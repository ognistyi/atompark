{
  "name": "ognistyi/atompark",
  "description": "atompark SMS API wrapper",
  "version": "0.1",
  "keywords": [
    "atompark api",
    "SMS service"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Andrew Ognistyi",
      "email": "ognistyi@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.0",
    "illuminate/support": "~5.1|~6.0",
    "illuminate/config": "~5.1|~6.0",
    "ext-simplexml": "*",
    "ext-json": "*",
    "ext-curl": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "4.7.*"
  },
  "autoload": {
    "psr-4": {
      "Ognistyi\\AtomPark\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Ognistyi\\AtomPark\\AtomParkServiceProvider"
      ],
      "aliases": {
        "AtomPark": "Ognistyi\\AtomPark\\AtomParkFacade"
      }
    }
  }
}